#include <string>
#include <vector>

using namespace std;

string solution(string n_str) {
    string answer = "";
    
    for(int i = 0; i < n_str.size(); i++){
        if(n_str[i] != '0') {
            answer += string(n_str.begin() + i, n_str.end());
            break;
        }
    }
    
    return answer;
}
