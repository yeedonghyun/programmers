#include <string>
#include <iostream>

using namespace std;

bool solution(string s)
{
    int Pcnt = 0;
    int Ycnt = 0;
    
    for(int i = 0; i < s.length(); i++){
        if(s[i] == 'p' || s[i] == 'P') Pcnt++;
        if(s[i] == 'y' || s[i] == 'Y') Ycnt++;
    }
    
    return Pcnt == Ycnt;
}
